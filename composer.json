{
    "name": "dimaspace/vkapi",
    "type": "library",
    "description": "Laravel helper for performing requests to VK API",
    "keywords": [
        "bafoed",
        "vkapi",
        "vk",
        "laravel"
    ],
    "homepage": "https://github.com/bafoed/laravel-vkapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Lempinen",
            "email": "bafoed@bafoed.ru",
            "homepage": "http://bafoed.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "insign/laravel-decaptcha": "~1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "dimaspace\\VKAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dimaspace\\VKAPI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
